{
  "name": "aio-store",
  "version": "2.2.2",
  "description": "A javascript app state management",
  "keywords": [
    "react",
    "store",
    "proxy",
    "state",
    "management"
  ],
  "files": [
    "./**"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "npm ci && npm run build:preview",
    "build:preview": "npm run clean && npm run lint:prettier && npm run build:webpack && npm run build:dts",
    "lint:prettier": "npm run lint && npm run prettier",
    "build:dts": "dts-bundle-generator --config ./dts.json && npm run clean:temp",
    "build:webpack": "webpack --config webpack.config.dev.ts && webpack --config webpack.config.prod.ts",
    "build:webpack:dev": "webpack --config webpack.config.dev.ts -w",
    "build:webpack:prod": "webpack --config webpack.config.prod.ts -w",
    "start": "npm run clean && npm run lint && concurrently npm:build:webpack:dev npm:build:webpack:prod npm:build:dts",
    "prettier": "prettier ./src --write",
    "lint": "eslint ./src --ext .ts",
    "bpv": "npm version preminor --no-git-tag-version",
    "clean": "npm run clean:lib:win && npm run clean:lib:unix",
    "clean:temp": "npm run clean:temp:win && npm run clean:temp:unix",
    "clean:check:win": "node -e \"if (process.platform === 'win32') process.exit(1)\"",
    "clean:check:unix": "node -e \"if (process.platform !== 'win32') process.exit(1)\"",
    "clean:lib:win": "npm run clean:check:win || , if exist lib rmdir /Q /S lib",
    "clean:lib:unix": "npm run clean:check:unix || rm -rf lib",
    "clean:temp:win": "npm run clean:check:win || , if exist temp rmdir /Q /S temp",
    "clean:temp:unix": "npm run clean:check:unix || rm -rf temp",
    "npm:pack": "npm pack",
    "test": "vitest",
    "test-coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klm-lab/store.git"
  },
  "author": "Arnaud Litaaba <litaabaarnaud@gmail.com>",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/klm-lab/store/issues"
  },
  "homepage": "https://github.com/klm-lab/store#readme",
  "devDependencies": {
    "@types/react": "^18.2.20",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitest/coverage-v8": "^0.34.4",
    "concurrently": "^8.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "dts-bundle-generator": "^8.0.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "3.0.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
